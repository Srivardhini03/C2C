<%- include('partials/header') %>

    <div class="col-md-6 col-sm-12">
      <form class="bankform" action="/users/profile/<%=locals.user._id%>/bankDetails" method="post">
        <h3 class="username">Bank Details</h3>
        <input name="name" type="text" placeholder="Your name" class="box"><br>
        <input name="accNo" type="text" placeholder="Account number" class="box"><br>
        <input name="ifsc"type="text" placeholder="IFSC Code" class="box"><br>
        <input name="email" type="email" placeholder="Your email" class="box"><br>
        <input name="num" type="number" placeholder="Contact number" class="box"><br>
        <button type="submit" name="button" class="btn btn-1">Submit</button>
      </form>

    </div>

<%- include('partials/footer') %>
